<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
	<settings>
		<!-- <setting name="mapUnderscoreToCamelCase" value="true" /> -->
		<setting name="jdbcTypeForNull" value="NULL" />
	</settings>
	<typeAliases>
		<typeAlias type="com.ssafy.model.dto.Food" alias="food" />
		<typeAlias type="com.ssafy.model.dto.Member" alias="member" />
		<typeAlias type="com.ssafy.model.dto.AteFood" alias="atefood" />
		<typeAlias type="com.ssafy.model.dto.getAte" alias="getate" />
		<typeAlias type="com.ssafy.model.dto.Board" alias="board" />
		<typeAlias type="com.ssafy.model.dto.aBoard" alias="aboard" />
		<typeAlias type="com.ssafy.model.dto.qBoard" alias="qboard" />
		<typeAlias type="com.ssafy.model.dto.ExpFood" alias="expfood" />
	</typeAliases>
	<mappers>
		<mapper resource="mybatis/foodmapper.xml" />
		<mapper resource="mybatis/membermapper.xml" />
		<mapper resource="mybatis/atefoodmapper.xml" />
		<mapper resource="mybatis/expfoodmapper.xml" />
		<mapper resource="mybatis/boardmapper.xml" />
		<mapper resource="mybatis/aboardmapper.xml" />
		<mapper resource="mybatis/qboardmapper.xml" />
	</mappers>
</configuration>
